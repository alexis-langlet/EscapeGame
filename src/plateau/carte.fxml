<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" onMouseEntered="#showprops" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plateau.CarteController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/grenier2.jpg" />
         </image>
         <effect>
            <BoxBlur height="15.0" iterations="3" width="15.0" />
         </effect>
      </ImageView>
      <ImageView fx:id="map" fitHeight="625.0" fitWidth="1147.0" layoutX="68.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/carte.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="mapicon" fitHeight="60.0" fitWidth="60.0" nodeOrientation="RIGHT_TO_LEFT" onMouseClicked="#mapIconClick" pickOnBounds="true" preserveRatio="true" rotate="180.0">
         <image>
            <Image url="@../images/back arrow.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="bigben" fitHeight="126.0" fitWidth="74.0" layoutX="289.0" layoutY="468.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../images/BigBen.png" />
         </image>
      </ImageView>
      <ImageView fx:id="toweroflondon" fitHeight="73.0" fitWidth="67.0" layoutX="1114.0" layoutY="302.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../images/TowerOfLondon.png" />
         </image>
      </ImageView>
      <ImageView fx:id="train" fitHeight="60.0" fitWidth="138.0" layoutX="124.0" layoutY="133.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../images/train.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
